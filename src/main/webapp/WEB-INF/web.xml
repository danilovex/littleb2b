<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>com.example.HelloServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>imagemGoogle</servlet-name>
    <servlet-class>com.example.ImagemGoogle</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EnviarMail</servlet-name>
    <servlet-class>com.example.EnviarMail</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>imagemGoogle</servlet-name>
    <url-pattern>/ImagemGoogle</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EnviarMail</servlet-name>
    <url-pattern>/EnviarMail</url-pattern>
  </servlet-mapping>
</web-app>